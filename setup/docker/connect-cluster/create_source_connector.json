{
  "name": "postgresql-sales-source-connector-01",
  "config": {
    "name": "postgresql-sales-source-connector-01",
    "connector.class": "JdbcSourceConnector",
    "connection.url": "jdbc:postgresql://source-database:5432/irdappdb",
    "connection.user": "connectuser",
    "connection.password": "secretdayo",
    "topic.prefix": "source-db-",
    "table.whitelist": "ticket_orders",
    "mode": "timestamp",
    "timestamp.column.name": "created_timestamp",
    "validate.non.null": true,
    "transforms": "createKey,extractInt",
    "transforms.createKey.type": "org.apache.kafka.connect.transforms.ValueToKey",
    "transforms.createKey.fields": "order_id",
    "transforms.extractInt.type": "org.apache.kafka.connect.transforms.ExtractField$Key",
    "transforms.extractInt.field": "order_id"
  }
}
